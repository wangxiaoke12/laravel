/*问答列表*/
.banner{height: 200px;padding-top: 100px;text-align: center;}
.banner span{color: #fff;font-size: 40.46px;display: block;margin-bottom: 55px;}
.banner a{color: #fff;font-size:14px;padding:12px 50px;border-radius:5px;border:1px solid #fff; background: rgba(0, 0, 0, 0.25);}
/*问答列表左侧*/
.content_left{padding:17px 20px;border:#e7e7e7 1px solid ;width: 818px;border-radius: 5px;}
/*问答列表TAB*/
.content_left .title{display: table;width: 100%;padding-bottom: 12px;border-bottom: #efefef 2px solid}
.content_left .title ul{}
.content_left .title ul li{float: left;margin-right: 2px;cursor: pointer;border-left: 2px #efefef solid;}
.content_left .title ul li:first-child{border-left: 0px #efefef solid;}
.content_left .title ul li span{font-size: 18px;color: #156bad;padding:0px 28px 12px 28px;}
.content_left .title ul li.active span{border-bottom:#156bad 2px solid ;}
.content_left .title ul li:hover span{border-bottom:#156bad 2px solid ;}
/**/
.content_left .qa_content{}
.content_left .qa_content .box{display: none}
.content_left .qa_content ul{}
.content_left .qa_content ul li{padding-top: 20px;padding-bottom: 20px;}
.content_left .qa_content ul li .{}

/*问答内容*/
.article_qa{border:#efefef solid 1px;border-radius: 5px;padding:28px 68px 19px 30px;width: 1100px;margin-bottom: 20px;display: table;}
.article_qa .article_content h1{color:#595757;font-size: 24px; }
.article_qa .article_content{max-width: 830px;float: left;}

.article_qa .article_content article{color:#4c4948;font-size: 14px;margin-top: 10px;line-height: 24px;position: relative;overflow: hidden ;height: 72px;}
.article_qa .article_content .article_show{color: #156bad;font-size: 12px; /*display: none*/ ;margin-left: 5px;}
.article_qa .article_content .existence_img{padding-left: 27px;background: url(/images/article/existence_img.png)no-repeat;}
.article_qa .article_lg{float: right;}
.article_qa .article_lg ul{}
.article_qa .article_lg ul li{float: left;padding:0px 12px;cursor:default; }
.article_qa .article_lg ul li:first-child{border-right: 1px #efefef solid }
.article_qa .article_lg ul li span{display: block;margin-bottom: 10px;color: #4c4948;font-size: 16px;}
.article_qa .article_lg ul li a{color: #156bad;font-size: 16px;}
#content_ellipsis{position: absolute;right: 0;top: 48px;background: #fff;cursor:pointer;}
.article_img{height: 405px;width: 605px;/*padding: 5px;*/margin: 20px 0 10px 0;/*box-shadow: 0px 0px 2px 2px #e0e0e0*/}
.article_img .large{height:332px;width: 605px;overflow: hidden;background: #535353}
.article_img .large ul{}
.article_img .large ul li{height:332px;width: 605px;float: left;display:none;color:#fff;}
.article_img .large ul li.active{display:block}
.article_img .large ul li img{height:332px;width: 605px;}
.article_img .small{height:60px;width: 605px;margin-top: 13px;}
.article_img .small ul{}
.article_img .small ul li{height:60px;width: 60px;margin-left:8px;background: #535353;float: left;}
.article_img .small ul li:first-child{margin-left:0px;}
.article_img .small ul li img{height:60px;width: 60px;}
.article_img .small ul li.active{opacity: 0.7;}

.article_bottom{margin-top: 23px;height: 30px;width: 830px;}
.article_bottom span{color:#9fa0a0;cursor:pointer;}
.article_bottom .time{font-size: 12px;line-height: 30px;}
.article_bottom .share{font-size: 12px;line-height: 30px;float: right;width: 76px;height: 30px;text-align: center;margin-right: 30px;position:relative;}
.article_bottom .share i{margin-right: 5px;}
.article_bottom .share:hover .share_menu{display: block}
/*分享*/
.article_bottom .share .share_menu{position:absolute;width: 107px;border: 1px solid #bfbfbf;    margin-left: -30px;display: none;padding: 5px 0px; }
.article_bottom .share .share_menu::before{content: ' ';border: 8px solid #bfbfbf; border-color: transparent transparent #bfbfbf;position: absolute;top:-16px;right:27px; }
.article_bottom .share .share_menu::after{content: ' ';border: 8px solid #fff; border-color: transparent transparent #fff;position: absolute;top:-15px;right:27px; }
.article_bottom .share .share_menu li{height: 34px;line-height: 34px;background: #fff;padding-left: 9px;text-align: left;}

.article_bottom .share .share_menu li img{height: 23px;width:23px;line-height: 34px;margin-top: 5.5px;float: left;}

.article_bottom .share .share_menu li span{color: #595757;font-size: 12px;margin-left: 5px;}
.article_bottom .share .share_menu li:hover span{color: #156bad}
.article_bottom .follow{font-size: 14px;line-height: 28px;float: right;width: 88px;height: 28px;border:#156bad 1px solid ;text-align: center;border-radius: 5px;margin-right: 30px;color: #156bad}
.article_bottom .follow:hover{color: #fff;background: #156bad}
.article_bottom .edit{font-size: 14px;line-height: 30px;float: right;width: 88px;height: 28px;border:#156bad 1px solid ;text-align: center;border-radius: 5px;color: #156bad}
.article_bottom .edit:hover{color: #fff;background: #156bad}
.article_bottom .up{font-size: 12px;line-height: 30px;float: right;width: 50px;text-align: center;display: none}
.comment .article_comment_form{display: none}

.comment{float: left;}
.content_right{float: left;}